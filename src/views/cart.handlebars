<div id="app">

  <h1 >Cart with the identification
    <span id="cartId">{{cart._id}}</span></h1>
  <div id="cartDetail">
    {{#each cart.products}}
      <div
       
        id="{{this.productId._id}}"
      >
        <div >
          <div class="px-5 mt-2">
            <h5
              
            >{{this.productId.title}}</h5>
            <p>Quantity: {{this.quantity}}</p>
            <p >Price: ${{this.productId.price}}</p>
            <div class="flex justify-between">
            {{!-- <p class="text-lg tracking-wider text-white my-1"
            {{!-- >Total price: ${{multiply this.productId.price this.quantity}}</p> --}}
            </div>
          </div>
        </div>

      </div>
    {{/each}}
  </div>
 
<script
  src="
https://cdn.jsdelivr.net/npm/sweetalert2@11.7.3/dist/sweetalert2.all.min.js
"
></script>
<script src="/js/cart.js"></script>